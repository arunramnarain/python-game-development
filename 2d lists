rows = int(input("enter the number of rows: "))
cols = int(input("enter the number of collumns: "))
matrix = []
for i in range(rows):
    temp = []
    for j in range(cols):
        x = int(input("enter your elements: "))
        temp.append(x)
    matrix.append(temp)

print("")
#to print items in matrix form
for i in range(rows):
    for j in range(cols):
        print(matrix[i][j],end = " ")
    print("\n")

